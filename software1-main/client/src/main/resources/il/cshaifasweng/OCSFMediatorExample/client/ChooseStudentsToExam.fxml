<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="SelectAllBu" prefHeight="680.0" prefWidth="936.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.ChooseStudentsToExamController">
    <children>
        <TableView fx:id="students_list_to_exam" layoutX="119.0" layoutY="104.0" prefHeight="214.0" prefWidth="503.0">
            <columns>
                <TableColumn fx:id="studentIdColumn" prefWidth="180.0" text="STUDENT ID" />
                <TableColumn fx:id="studentFirstNameColumn" prefWidth="172.0" text="STUDENT FIRST NAME" />
                <TableColumn fx:id="studentLastNameColumn" prefWidth="150.0" text="STUDENT LAST NAME" />
            </columns>
        </TableView>
        <Label fx:id="typeoftheexamLAB" layoutX="61.0" layoutY="355.0" prefHeight="18.0" prefWidth="171.0" text="Select Execution Way:">
            <font>
                <Font name="System Bold" size="16.0" />
            </font></Label>
        <RadioButton fx:id="manualexamRB" layoutX="73.0" layoutY="398.0" mnemonicParsing="false" onAction="#on_manualexamRB_selected" text="Manual Exam">
            <font>
                <Font size="14.0" />
            </font></RadioButton>
        <RadioButton fx:id="computerizedexamRB" layoutX="73.0" layoutY="439.0" mnemonicParsing="false" onAction="#on_computerizedexamRB_selected" prefHeight="25.0" prefWidth="108.0" text="digital Exam">
            <font>
                <Font size="14.0" />
            </font></RadioButton>
        <Button fx:id="executeBT" layoutX="207.0" layoutY="529.0" mnemonicParsing="false" onAction="#OnExecuteClicked" text="Execute">
            <font>
                <Font size="14.0" />
            </font></Button>
        <CheckBox fx:id="SelectAllCheckBox" layoutX="61.0" layoutY="69.0" mnemonicParsing="false" onAction="#SelectAll" prefHeight="20.0" prefWidth="150.0" text="Select All Students">
            <font>
                <Font name="System Bold" size="14.0" />
            </font></CheckBox>
        <Button fx:id="executeBT1" layoutX="14.0" layoutY="636.0" mnemonicParsing="false" onAction="#OnCancelClicked" text="Cancel">
            <font>
                <Font size="14.0" />
            </font></Button>
        <TextField fx:id="execution_code_tf" layoutX="193.0" layoutY="488.0" prefHeight="26.0" prefWidth="94.0" />
        <Label fx:id="typeoftheexamLAB1" layoutX="61.0" layoutY="490.0" prefHeight="21.0" prefWidth="127.0" text="Execution Code :">
            <font>
                <Font name="System Bold" size="15.0" />
            </font></Label>
        <Button fx:id="logout" layoutX="853.0" layoutY="636.0" mnemonicParsing="false" onAction="#on_logOut_clicked" text="Log Out">
            <font>
                <Font size="14.0" />
            </font></Button>
    </children>
</AnchorPane>