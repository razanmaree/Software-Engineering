<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="680.0" prefWidth="936.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.ExamResultsForMangerController">
    <children>
        <Label layoutX="52.0" layoutY="47.0" prefHeight="26.0" prefWidth="220.0" text="Show Exam Results For Exam:">
            <font>
                <Font name="System Bold" size="15.0" />
            </font></Label>
        <Label layoutX="52.0" layoutY="104.0" text="Subject Name:">
            <font>
                <Font name="System Bold" size="15.0" />
            </font></Label>
        <Label layoutX="54.0" layoutY="162.0" text="Course Name:">
            <font>
                <Font name="System Bold" size="15.0" />
            </font></Label>
        <TableView fx:id="resultsTable" layoutX="71.0" layoutY="248.0" prefHeight="283.0" prefWidth="794.0">
            <columns>
                <TableColumn fx:id="firstNameC" prefWidth="151.0" text="STUDENT FIRST NAME" />
                <TableColumn fx:id="lastNameC" prefWidth="143.0" text="STUDENT LAST NAME" />
                <TableColumn fx:id="scoreC" minWidth="0.0" prefWidth="97.0" text="FINAL SCORE" />
                <TableColumn fx:id="durationC" prefWidth="114.0" text="EXAM DURATION" />
                <TableColumn fx:id="timeC" prefWidth="132.0" text="EXAM ACTUAL TIME" />
                <TableColumn fx:id="exec_code" prefWidth="155.0" text="EXAM EXECUTION CODE" />
            </columns>
        </TableView>
        <Button fx:id="backB" layoutX="14.0" layoutY="636.0" mnemonicParsing="false" onAction="#back_button_clicked" text="Back">
            <font>
                <Font size="14.0" />
            </font></Button>
        <Label fx:id="examCodeTF" layoutX="290.0" layoutY="47.0" prefHeight="26.0" prefWidth="41.0" text="0">
            <font>
                <Font size="15.0" />
            </font></Label>
        <Label fx:id="subjectTF" layoutX="201.0" layoutY="101.0" prefHeight="26.0" prefWidth="220.0" text="&quot;&quot;" />
        <Label fx:id="courseTF" layoutX="201.0" layoutY="160.0" prefHeight="26.0" prefWidth="220.0" text="&quot;&quot;" />
        <Button fx:id="logout" layoutX="853.0" layoutY="636.0" mnemonicParsing="false" onAction="#on_logOut_clicked" text="Log Out">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
    </children>
</AnchorPane>